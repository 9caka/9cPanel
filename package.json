{
  "name": "9cpanel",
  "version": "1.11.3",
  "description": "Panel de d√©veloppement et Gaming",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "9cBinks",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "electron-log": "^5.4.2",
    "electron-updater": "^6.6.2",
    "js-yaml": "^4.1.0",
    "os-utils": "^0.0.14",
    "rss-parser": "^3.13.0",
    "semver": "^7.7.2",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.27.7",
    "tailwindcss": "^3.4.1",
    "xml2js": "^0.6.2"
  },
  "build": {
    "appId": "com.9cBinks.9cpanel",
    "productName": "9cpanel",
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "files": [
      "main.js",
      "src/preload.js",
      "src/index.html",
      "src/assets/**/*",
      "src/data/icons.json",
      "src/data/rss-feeds.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "publish": {
      "provider": "github",
      "owner": "9caka",
      "repo": "9cPanel"
    }
  }
}
